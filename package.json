{
  "name": "storybook-visualizor",
  "version": "0.0.1",
  "description": "visualize components for Marble Marketplace projcet",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/nakayamaiwao/storybook",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf ./dist && rollup -c && tsc -d --emitDeclarationOnly --declarationDir dist",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check --loglevel error 'src/**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest",
    "release": "yarn build && np"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format:write",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@pancakeswap-libs/eslint-config-pancake": "0.1.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@rollup/plugin-url": "^6.0.0",
    "@storybook/addon-a11y": "^6.1.18",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-essentials": "^6.1.18",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/react": "^6.1.18",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.5",
    "@types/react-transition-group": "^4.4.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "^7.20.0",
    "husky": "4.3.5",
    "jest-styled-components": "^7.0.3",
    "np": "^7.4.0",
    "prettier": "^2.1.2",
    "rollup": "^2.39.0",
    "styled-components": "^5.2.0",
    "themeprovider-storybook": "^1.6.4",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "styled-components": "^5.2.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.6.1",
    "@splinetool/react-spline": "^2.2.6",
    "@types/lodash": "^4.14.162",
    "@types/react": "^18.2.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-system": "^5.1.10",
    "axios": "^1.4.0",
    "jest": "^29.5.0",
    "lodash": "^4.17.20",
    "near-api-js": "^2.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-is": "^18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-router": "^6.11.1",
    "react-router-dom": "^6.11.1",
    "react-transition-group": "^4.4.5",
    "styled-system": "^5.1.5",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0"
  }
}
